breed [ birds bird ]
breed [ leaders leader]
globals [ choice the-leader ]
extensions [csv]

;;;;;;;;;;;;;;;;;
;;Crear turtles;;
;;;;;;;;;;;;;;;;;

; Crear el mundo y los pingüinos
to setup
  clear-all
  ; dividir el mundo por la diagonal
  ; un lado sera blanco y otro azul
  ask patches [ if pxcor + pycor < 1.9 [set pcolor 84 - random-float 0.45]]
  ask patches with [ not shade-of? 84 pcolor ][ set pcolor white   ]
  set-default-shape birds "bird"
  create-birds Numero_pinguinos [ ;; puedes cambiar el numero de punguinos con un deslizador
    set size 2
    set color black + random 7
    setxy random-xcor random-ycor
  ]
  reset-ticks
end

; Crear el pez
to setup-leader
  create-leaders 1
  [ set the-leader self
    set size 2
    set color 65
    set shape "fish"
    setxy -10 -8
  ]
end

;;;;;;;;;;;;;
;; ACCIONES;;
;;;;;;;;;;;;;

;; MOVIMIENTO ALEATORIO
to go-aleatorio
  ask turtles [wiggle forward 0.1
    ; accion de wiggle y avanzar
    ]
  tick
  csv:to-file "aleatorio.csv" [ (list size xcor ycor) ] of turtles
end

to wiggle
  right random 80
  left random 80
end

;; MOVIMIENTO AGRUPADO
to go-agrupado
  if not any? turtles with [ shape = "fish" ] ;; si no se ha pulsado Crear Pez, crearlo
  [setup-leader] ;; crear lider (pez)
  move
  move-leader
  csv:to-file "agrupado.csv" [ (list size xcor ycor) ] of turtles
end

;; Movimiento de los pingüinos
to move
  ask birds
  [face the-leader ; orientación hacia el lider (pez)
    ifelse distance the-leader > 0.5
    [fd 0.02]
    [move-to the-leader] ; los pingüinos se mueven hacia el lider
  ]
end

;; Movimiento del lider (pez)
to move-leader
  ; El pez se mueve solo en el mar
  ; si llega a la zona blanca, da media vuelta
  ask the-leader [ ifelse [pcolor] of patch-ahead 1 = white [ lt random-float 360]
  ; movimiento del lider
    [ right random 10
    left random 10
      forward 0.019] ]

end

;; MOVIMIENTO REGULAR/UNIFORME
to go-regular
  ; separacion de pinguinos con un radio minimo de 5
  ask birds [ while [ any? other turtles in-radius 5] [setxy random-xcor random-ycor ]
  set heading 70 ; direccion en la que quiero que se muevan los pinguinos
  fd 0.0008]
  csv:to-file "regular.csv" [ (list size xcor ycor) ] of turtles
end
